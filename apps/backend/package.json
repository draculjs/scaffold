  "scripts": {
    "back": "nodemon src/index.js --exec node -e js",
    "backBabel": "BABEL_DISABLE_CACHE=1 nodemon src/index.js --exec babel-node -e js",
    "init": "BABEL_DISABLE_CACHE=1 babel-node  src/init-script.js",
    "build": "./node_modules/.bin/babel src -d dist --copy-files && ./node_modules/.bin/babel emails -d dist/emails --copy-files && cp media dist/ -r && cp .env.example dist/.env && cp package.json dist/package.json",
    "buildpm2": "./node_modules/.bin/babel src -d ../../dist --copy-files && ./node_modules/.bin/babel emails -d ../../dist/emails --copy-files && cp media ../../dist/ -r && cp web ../../dist/ -r  && cp package.json ../../dist/package.json && cp runpm2.sh ../../dist/runpm2.sh",
